{
	"name": "@sveltejs-labs/chart.js",
	"version": "1.1.0",
	"description": "Chart.js wrapper for Svelte",
	"keywords": [
		"svelte",
		"sveltekit",
		"wrapper",
		"chart",
		"chart.js"
	],
	"homepage": "https://www.npmjs.com/package/@sveltejs-labs/chart.js",
	"author": "Minsoo Choo <minsoochoo0122@proton.me>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltejs-labs/Chart.js.git"
	},
	"bugs": {
		"url": "https://github.com/sveltejs-labs/Chart.js/issues"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && bun run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest run",
		"test:headless": "vitest run --browser.headless",
		"test:watch": "vitest",
		"test:watch:headless": "vitest --browser.headless",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"chart.js": "^4.4.8",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.6",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"@playwright/test": "^1.51.1",
		"@storybook/addon-essentials": "^8.6.9",
		"@storybook/addon-svelte-csf": "^5.0.0-next.28",
		"@storybook/blocks": "^8.6.9",
		"@storybook/experimental-addon-test": "^8.6.9",
		"@storybook/manager-api": "^8.6.9",
		"@storybook/svelte": "^8.6.9",
		"@storybook/sveltekit": "^8.6.9",
		"@storybook/test": "^8.6.9",
		"@storybook/theming": "^8.6.9",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/chart.js": "^2.9.41",
		"@types/node": "^22.13.13",
		"@vitest/browser": "^3.0.9",
		"@vitest/coverage-v8": "^3.0.9",
		"@vitest/ui": "^3.0.9",
		"chart.js": "^4.4.8",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-storybook": "^0.12.0",
		"eslint-plugin-svelte": "^3.3.3",
		"globals": "^16.0.0",
		"playwright": "^1.51.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.9",
		"storybook": "^8.6.9",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.28.0",
		"vite": "^6.2.3",
		"vitest": "^3.0.9",
		"vitest-browser-svelte": "^0.1.0"
	}
}
